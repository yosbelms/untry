{
  "name": "untry",
  "version": "0.0.5",
  "description": "Better error handling in JavaScript",
  "keywords": ["error", "try-catch", "handling"],
  "repository": "https://github.com/yosbelms/untry.git",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc --sourceMap -d",
    "watch": "tsc --sourceMap --watch",
    "clean": "find . -type f \\( -name \"*.js\" -o -name \"*.js.map\" -o -name \"*.d.ts\" -o -name \"*.d.ts.map\" \\) | grep -v \"./node_modules\" | xargs rm",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "author": "Yosbel Marin",
  "license": "MIT",
  "dependencies": { },
  "devDependencies": {
    "typescript": "^5.1.3",
    "@types/node": "^20.3.1"
  }
}
